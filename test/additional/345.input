>> RM -FORCE TEST_HDF5
>> IF ( $MOLCAS_DRIVER = UNKNOWN_VARIABLE )
>>   EXPORT MOLCAS_DRIVER=molcas
>> ENDIF
>> SHELL $MOLCAS_DRIVER have_feature hdf5 || touch TEST_HDF5
>> EXPORT HAVE_HDF5=1
>> IF ( -FILE TEST_HDF5 )
>>   EXPORT HAVE_HDF5=0
>> ENDIF

>export MOLCAS_REDUCE_PRT=NO
>export MOLCAS_NOCHECK=MLTPL


>COPY velocity.xyz  $Project.velocity.xyz
>COPY C3H6N.xyz .
&GATEWAY
COORD=C3H6N.xyz
BASIS= 3-21G
GROUP= nosym
NoCD

> EXPORT MOLCAS_MAXITER=3
> DOWHILE

&SEWARD

&SCF
Charge= 1

&ALASKA
CutOff= 1.0D-8

&DYNAMIX
VelVer
DT= 10.0
VELO= 1
THERMO= 2
> END DO

