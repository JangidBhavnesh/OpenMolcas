&GATEWAY
  Coord = PSB2-A.xyz
  Coord = PSB2-B.xyz
  BSSE = 2
  Basis = ANO-R1
  NoMove
  Group = Nosym

&SEWARD
  FakeRICD

> COPY $Project.RunFile AUXRFIL2

