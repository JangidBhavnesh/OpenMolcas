***** SYSTEM A ******

&GATEWAY
  Coord = PSB2-A.xyz
  Basis = ANO-R1
  NoMove
  Group = NoSym
  RICD

&SEWARD

&SCF
  Charge = 1

&RASSCF
  File = $Project.scf.h5
  Charge = 1
  NActEl = 4
  RAS2 = 4
  StAverage = 3
  Alter = 2
    1 13 14
    1 17 20

&CASPT2
  File = $Project.rasscf.h5
  Multistate = all

> COPY $Project.caspt2.h5 MonA.caspt2.h5

* System B is identical but translated 3.5 Å
> EXEC $MOLCAS/Tools/mort/mort.py -t 0.0 0.0 6.6140414362 MonA.caspt2.h5 MonB.caspt2.h5

