  Coord = PSB2-A.xyz
  Coord = PSB2-B.xyz
  BSSE = 1
  Basis = ANO-R1
  NoMove
  Group = NoSym
  RICD

&SEWARD

> COPY $Project.RunFile  AUXRFIL1

***** SYSTEM A ******

&GATEWAY
  Coord = PSB2-A.xyz
  Basis = ANO-R1
  NoMove
  Group = NoSym

&SEWARD
